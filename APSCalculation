package cputapsscore;


import java.util.Scanner;
public class CPUTAPSscore {
    /* Calculate APS marks of the the six subjects */
    
    class APS{

    
    public static void main(String[] args) {
       String  Homelang = null;
       String FAL = null;
       String Other1 = null;
       String Other2 = null;
       String Other3 = null;
       String Other4 = null;

       System.out.println("Enter Percentage of each of the six subject excluding LO");
       Scanner r=new Scanner(System.in);
       Homelang=r.next(Homelang);
       FAL=r.next(FAL);
       Other1=r.next(Other1);
       Other2=r.next(Other2);
       Other3=r.next(Other3);
       Other4=r.next(Other4);
       
       String sum=Homelang+FAL+Other1+Other2+Other3+Other4;
       System.out.println("Total Marks"+sum);
       
       double APS=sum/10.0;
       System.out.println("Average marks"+ APS);
    }
    }
    
}
