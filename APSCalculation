            this.others2subPercent = 3;
        }else if(others2subPercent >= 50 && others2subPercent <= 59){
            this.others2subPercent = 4;
        }else if(others2subPercent >= 60 && others2subPercent <= 69){
            this.others2subPercent = 5;
        }else if(others2subPercent >= 70 && others2subPercent <= 79){
            this.others2subPercent = 6;
        }else if(others2subPercent >= 80 && others2subPercent <= 89){
            this.others2subPercent = 7;
        }else if(others2subPercent >= 90 && others2subPercent <=100){
            this.others2subPercent = 8;
        }
    }
     
       public void CPUTapsScore6(double other3subPercent){
        if(other3subPercent < 20){
            this.other3subPercent = 0;
        }else if(other3subPercent >= 20 && other3subPercent<= 29){
            this.other3subPercent = 1;
        }else if(other3subPercent >= 30 && other3subPercent <= 39){
            this.other3subPercent = 2;
        }else if(other3subPercent >= 40 && other3subPercent <= 49){
            this.other3subPercent = 3;
        }else if(other3subPercent >= 50 && other3subPercent <= 59){
            this.other3subPercent = 4;
        }else if(other3subPercent >= 60 && other3subPercent <= 69){
            this.other3subPercent = 5;
        }else if(other3subPercent >= 70 && other3subPercent <= 79){
            this.other3subPercent = 6;
        }else if(other3subPercent >= 80 && other3subPercent <= 89){
            this.other3subPercent = 7;
        }else if(other3subPercent >= 90 && other3subPercent <=100){
            this.other3subPercent = 8;
        }
    
       }
       
       public double pointTotal(){
        double total = homelanguagePercent + firstadditionallaguagePercent + mathsPercent + lifeorientationPercent + other1subPercent + others2subPercent +
        other3subPercent;
        Math.round(total);
        return total;
       }
}
      